# Voice Changer Better 环境变量配置

# 服务器配置
EX_PORT=18888
EX_TB_PORT=16006

# GPU配置
USE_GPU=on
# 可选值: on|off

# 用户ID配置（Linux/Mac）
LOCAL_UID=1000
LOCAL_GID=1000

# Docker镜像配置
DOCKER_IMAGE=voice-changer-better:latest

# 模型路径配置
MODEL_DIR=./models
PRETRAIN_DIR=./pretrain
TMP_DIR=./tmp

# 日志配置
LOG_LEVEL=INFO

# HTTPS配置
USE_HTTPS=false
SSL_CERT_PATH=
SSL_KEY_PATH=

# 跨域配置
CORS_ORIGINS=*

# 性能配置
MAX_WORKERS=4
SHM_SIZE=1024M

# 开发模式
DEV_MODE=false